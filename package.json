		"session",
		"gramio-session",
		"storage",
		"redis",
		"in-memory"
	],
	"devDependencies": {
		"@types/bun": "^1.1.6",
		"gramio": "^0.0.46",
		"pkgroll": "2.4.0",
		"typescript": "^5.5.4"
	},
	"peerDependencies": {
		"gramio": ">=0.0.39 <1.0.0"
