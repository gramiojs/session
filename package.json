{
	"name": "@gramio/session",
	"version": "0.1.5",
	"description": "Session plugin for GramIO",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"type": "module",
	"homepage": "https://github.com/gramiojs/session",
	"readme": "https://gramio.dev/",
